<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="TOP_CENTER" prefHeight="300.0" prefWidth="200.0" spacing="50.0" style="-fx-background-color: #595959;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.hivecampuscompany.hivecampus.state.javafx.controller.InitialJavaFXPageController">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="25.0" />
   </padding>
   <VBox alignment="TOP_RIGHT">
      <MenuButton mnemonicParsing="false">
         <graphic>
            <ImageView fx:id="imvLang" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" />
         </graphic>
         <items>
            <MenuItem fx:id="mibtnLangChange" mnemonicParsing="false">
               <graphic>
                  <ImageView fx:id="imvLangChange" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </MenuItem>
         </items>
      </MenuButton>
   </VBox>
   <Button fx:id="btnLoginChoice" mnemonicParsing="false" prefWidth="120.0" text="Label">
      <opaqueInsets>
         <Insets />
      </opaqueInsets>
   </Button>
   <Button fx:id="btnSignUpChoice" mnemonicParsing="false" prefWidth="120.0" text="Label" />
</VBox>
