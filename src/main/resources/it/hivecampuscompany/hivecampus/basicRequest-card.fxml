<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER_LEFT" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.hivecampuscompany.hivecampus.state.javafx.controller.ManageRequestsJavaFxPageController">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <VBox alignment="CENTER">
               <children>
                  <ImageView fx:id="imvUser" fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@images/account.png" />
                     </image>
                  </ImageView>
                  <HBox alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Text fx:id="txtName" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                        <Text fx:id="txtSurname" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox alignment="CENTER_LEFT">
               <children>
                  <Text fx:id="txtStart" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text fx:id="txtDuration" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text fx:id="txtEmail" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
                  <Text fx:id="txtPhoneNumber" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
            </VBox>
         </children>
      </HBox>
      <Text fx:id="txtMessage" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
      <Pane style="-fx-border-color: black; -fx-border-radius: 5;" VBox.vgrow="ALWAYS">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Label fx:id="lblMsgContent" text="Label">
               <padding>
                  <Insets bottom="10.0" left="10.0" top="10.0" />
               </padding>
            </Label>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="10.0" />
         </padding>
      </Pane>
   </children>
</VBox>
