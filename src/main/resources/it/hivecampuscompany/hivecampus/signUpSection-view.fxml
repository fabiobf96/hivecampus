<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" spacing="10.0" style="-fx-background-color: #595959;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.hivecampuscompany.hivecampus.state.javafx.controller.SignUpJavaFXPageController">
   <padding>
      <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
   </padding>
   <VBox alignment="TOP_RIGHT">
      <MenuButton mnemonicParsing="false">
         <graphic>
            <ImageView fx:id="imvLang" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" />
         </graphic>
         <items>
            <MenuItem fx:id="mibtnLangChange" mnemonicParsing="false">
               <graphic>
                  <ImageView fx:id="imvLangChange" fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true" />
               </graphic>
            </MenuItem>
         </items>
      </MenuButton>
   </VBox>
   <Region VBox.vgrow="ALWAYS" />
   <VBox alignment="CENTER" spacing="10.0">
      <Label fx:id="lblSignUp" text="Label" textFill="WHITE">
         <font>
            <Font name="Arial" size="20.0"/>
         </font>
      </Label>
      <HBox alignment="CENTER">
         <TextField fx:id="txfFirstName" maxWidth="260.0" prefHeight="26.0" prefWidth="127.5" promptText="Label"
                    style="-fx-background-radius: 7;" HBox.hgrow="ALWAYS">
            <font>
               <Font name="Arial" size="12.0"/>
            </font>
            <HBox.margin>
               <Insets right="5.0"/>
            </HBox.margin>
         </TextField>
         <TextField fx:id="txfLastName" maxWidth="260.0" prefHeight="25.0" prefWidth="127.5" promptText="Label"
                    style="-fx-background-radius: 7;" HBox.hgrow="ALWAYS">
            <font>
               <Font name="Arial" size="12.0"/>
            </font>
            <HBox.margin>
               <Insets left="5.0"/>
            </HBox.margin>
         </TextField>
      </HBox>
      <TextField fx:id="txfEmail" maxWidth="530.0" prefHeight="25.0" prefWidth="265.0" promptText="Label"
                 style="-fx-background-radius: 7;">
         <font>
            <Font name="Arial" size="12.0"/>
         </font>
      </TextField>
      <PasswordField fx:id="txfPassword" maxWidth="530.0" prefHeight="25.0" prefWidth="265.0" promptText="Label"
                     style="-fx-background-radius: 7;"/>
      <PasswordField fx:id="txfConfPassword" maxWidth="530.0" prefHeight="25.0" prefWidth="265.0" promptText="Label"
                     style="-fx-background-radius: 7;"/>
      <TextField fx:id="txfTelephone" maxWidth="530.0" prefHeight="25.0" prefWidth="265.0" promptText="Label"
                 style="-fx-background-radius: 7;">
         <font>
            <Font name="Arial" size="12.0"/>
         </font>
      </TextField>
      <VBox fillWidth="false" maxWidth="530.0" prefWidth="265.0">
         <Label fx:id="lblChoose" text="Label" textFill="WHITE">
            <font>
               <Font name="Arial" size="12.0"/>
            </font>
         </Label>
      </VBox>
      <HBox fillHeight="false" maxWidth="530.0" prefWidth="265.0" spacing="10.0">
         <CheckBox fx:id="ckbOwner" mnemonicParsing="false" text="Label" textFill="WHITE"/>
         <CheckBox fx:id="ckbTenant" mnemonicParsing="false" text="Label" textFill="WHITE"/>
      </HBox>
   </VBox>
   <Button fx:id="btnSignUp" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Label" />
   <HBox alignment="CENTER" fillHeight="false">
      <Label fx:id="lblAccount" text="Label" textFill="WHITE">
         <font>
            <Font name="Arial" size="12.0" />
         </font>
      </Label>
      <Button fx:id="btnLogHere" mnemonicParsing="false" style="-fx-background-color: transparent;" text="Label" textFill="#0bb7ed" underline="true" />
   </HBox>
   <Region VBox.vgrow="ALWAYS" />
</VBox>
